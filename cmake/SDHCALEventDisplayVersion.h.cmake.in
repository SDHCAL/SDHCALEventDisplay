#ifndef SDHCALEVENTDISPLAY_HH
#define SDHCALEVENTDISPLAY_HH

// version macros
#define SDHCALEventDisplay_MAJOR_VERSION @SDHCALEventDisplay_VERSION_MAJOR@
#define SDHCALEventDisplay_MINOR_VERSION @SDHCALEventDisplay_VERSION_MINOR@
#define SDHCALEventDisplay_PATCH_LEVEL @SDHCALEventDisplay_VERSION_PATCH@
#define SDHCALEventDisplay_VERSION "@SDHCALEventDisplay_VERSION_MAJOR@.@SDHCALEventDisplay_VERSION_MINOR@.@SDHCALEventDisplay_VERSION_PATCH@"

#define SDHCALEventDisplay_VERSION_GE( MAJV , MINV , PLEV )  ( (SDHCALEventDisplay_MAJOR_VERSION > MAJV) || ( (SDHCALEventDisplay_MAJOR_VERSION == MAJV) && (SDHCALEventDisplay_MINOR_VERSION > MINV) ) || ( (SDHCALEventDisplay_MAJOR_VERSION == MAJV) && (SDHCALEventDisplay_MINOR_VERSION == MINV) && (SDHCALEventDisplay_PATCH_LEVEL >= PLEV) ) )

#endif // SDHCALEVENTDISPLAY_HH
